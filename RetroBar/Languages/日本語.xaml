    <s:String x:Key="use_software_rendering">ソフトウェアレンダリングを使用(_U)</s:String>
    <s:String x:Key="ok_dialog">OK</s:String>

    <s:String x:Key="start_text">スタート</s:String>
    <s:String x:Key="start_text_xp">スタート</s:String>
