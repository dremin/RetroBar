<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <s:String x:Key="retrobar_properties">RetroBar 設定</s:String>
    <s:String x:Key="taskbar_tab">タスクバー</s:String>
    <s:String x:Key="advanced_tab">詳細設定</s:String>
    <s:String x:Key="taskbar_appearance">タスク バーのデザイン</s:String>
    <s:String x:Key="notification_area">通知領域</s:String>
    <s:String x:Key="autostart">ログオン時に自動的に起動する(_L)</s:String>
    <s:String x:Key="language_text">言語(_G):</s:String>
